{
  "name": "@outboundiq/express",
  "version": "0.1.0",
  "description": "OutboundIQ SDK for Express.js - Automatic outbound API monitoring",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "keywords": [
    "outboundiq",
    "express",
    "api",
    "monitoring",
    "observability",
    "http",
    "tracking"
  ],
  "author": "OutboundIQ",
  "license": "MIT",
  "dependencies": {
    "@outboundiq/core": "file:../outboundiq-js"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "express": "^4.18.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

